<aura:component >
    <aura:attribute name="vol" type="Volunteer__c"></aura:attribute>
	<div class="demo-only" style="width: 30rem;">
<article class="slds-tile">
<h3 class="slds-tile__title slds-truncate" title="{!'Volunteer Name =>' + v.vol.Name}"><a href="javascript:void(0);">{!v.vol.Name}</a></h3>
<div class="slds-tile__detail">
<dl class="slds-list_horizontal slds-wrap">
<dt class="slds-item_label slds-text-color_weak slds-truncate" title="First Label">Height:</dt>
<dd class="slds-item_detail slds-truncate" title="Description for first label">{!v.vol.Height__c}</dd>
<dt class="slds-item_label slds-text-color_weak slds-truncate" title="Second Label">Weight:</dt>
<dd class="slds-item_detail slds-truncate" title="Description for second label">{!v.vol.Weight__c}</dd>
    <dt class="slds-item_label slds-text-color_weak slds-truncate" title="BMI">BMI:</dt>
<dd class="slds-item_detail slds-truncate" title="Description for second label">{!v.vol.BMI__c}</dd>
    <dt class="slds-item_label slds-text-color_weak slds-truncate" title="Second Label">Category:</dt>
<dd class="slds-item_detail slds-truncate" title="Description for second label">{!v.vol.Category__c}</dd>
     <dt class="slds-item_label slds-text-color_weak slds-truncate" title="Second Label">Mobile:</dt>
<dd class="slds-item_detail slds-truncate" title="Description for second label">{!v.vol.Mobile__c}</dd>
     <dt class="slds-item_label slds-text-color_weak slds-truncate" title="Second Label">Email:</dt>
<dd class="slds-item_detail slds-truncate" title="Description for second label">{!v.vol.Email__c}</dd>
</dl>
</div>
</article>
</div>
</aura:component>